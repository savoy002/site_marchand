{% extends 'base.html.twig' %}

{% block title %} Image d'utilisateur. {% endblock %}


{% block body %}

	<h1>Changer l'image de l'utilisateur.</h1>

	{% if errors is defined %}
		<ul>
			{% for error in errors %}
				<li>{{ error }}</li>
			{% endfor %}
		</ul>
	{% endif %}

	{{ form(form) }}

{% endblock %}